    "eslint": "^8.49.0",
    "eslint-plugin-playwright": "^1.5.2",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.0.3",
    "typescript": "~5.4.0",
    "vite": "^5.1.6",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.6"

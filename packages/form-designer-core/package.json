    "codemirror": "^5.65.16",
    "codemirror-editor-vue3": "^2.1.7",
    "dayjs": "^1.11.10",
    "element-plus": "^2.2.28",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue3-barcode": "^1.0.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
